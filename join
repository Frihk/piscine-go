##join.go, basicjoin 

package main

import "fmt"

func Join(strs []string, sep string) string {
	var result string
	for i := range strs {
		result = result + strs[i] + sep
	}

	return result

}

func main() {
	toConcat := []string{"Hello!", " How", " are", " you?"}
	fmt.Println(Join(toConcat, ":"))
}
