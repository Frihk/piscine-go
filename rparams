package main

import (
	"github.com/01-edu/z01"
	"os"
)

func main() {

	arguments := os.Args
	var length int
	for l := range arguments {
		length = l
	}

	for i := length; i > 0; i-- {
		for _, j := range arguments[i] {
			z01.PrintRune(j)
		}
		z01.PrintRune('\n')
	}

}


package main

import (
	"os"

	"github.com/01-edu/z01"
)

func main() {

	for i := len(os.Args) - 1; i >= 1; i-- {
		for _, c := range os.Args[i] {
			z01.PrintRune(c)
		}
	}
}
